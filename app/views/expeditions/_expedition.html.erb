<div id="expedition_<%= expedition.id %>">
  <h2><%= expedition.title %></h2>
  <p><%= expedition.description %></p>
  <%= link_to 'Show', expedition_path(expedition) %>
  <%= link_to 'Delete', expedition_path(expedition), remote: true, method: :delete, data: { confirm: 'Are you certain you want to delete this?' } %>
</div>